{
  "name": "benchmarks",
  "version": "0.1.0",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "build:tsc": "pnpm -w tsc -b",
    "processCache": "pnpm build:tsc && node ./dist/processor.js",
    "ci": "node ./dist/wasm.js && node ./dist/hash-keys.js && PG_DISABLED=true node ./dist/query-list.js && PG_DISABLED=true node ./dist/series.js",
    "benchmark": "pnpm test-wasm && pnpm test-query-list && pnpm test-series && pnpm test-hash-keys",
    "test-wasm": "pnpm build:tsc && node ./dist/wasm.js",
    "test-query-list": "pnpm build:tsc && PG_CONNECTION_STRING=postgresql://postgres:password@localhost:5555/analytics node ./dist/query-list.js",
    "test-series": "pnpm build:tsc && PG_CONNECTION_STRING=postgresql://postgres:password@localhost:5555/analytics node ./dist/series.js",
    "test-hash-keys": "pnpm build:tsc && node ./dist/hash-keys.js"
  },
  "dependencies": {
    "@powerhousedao/analytics-engine-browser": "workspace:*",
    "@powerhousedao/analytics-engine-core": "workspace:*",
    "@powerhousedao/analytics-engine-knex": "workspace:*",
    "@powerhousedao/analytics-engine-pg": "workspace:*",
    "crc":"catalog:",
    "crc-32":"catalog:",
    "knex":"catalog:",
    "luxon":"catalog:",
    "pg": "catalog:",
    "tinybench": "catalog:"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "catalog:",
    "@types/luxon": "catalog:",
    "@types/node": "catalog:"
  }
}
